pipeline {
  agent any
  stages {
    stage('Clean Workspace'){
      steps {
        cleanWs()
      }
    }
    stage ('Checkout') {
      steps {
        checkout scm
      }
    }          
    stage('Testing...') {
    agent{
        docker{image 'postman/newman@1.0.0'}
      }
      steps{
        npm install newman
      }
    }
  }
}
