pipeline {
  agent any
  stages {
    stage('Clean Workspace'){
      steps {
        cleanWs()
      }
    }
    stage ('Checkout') {
      steps {
        checkout scm
      }
    }          
    stage('Testing...') {
    agent{
        docker{image 'postman/newman'}
      }
      steps{
      dockerImage.inside("--entrypoint=''") {
     echo "Tests passed"
}
      }
    }
  }
}
