pipeline {
  agent any
  stages {
    stage('Clean Workspace'){
      steps {
        cleanWs()
      }
    }
    stage ('Checkout') {
      steps {
        checkout scm
      }
    }          
    stage('Testing...') {
    agent{
        docker{
          image 'postman/newman:latest'
          reuseNode true}

      }
      steps{
        bat 'npm -v'
       // bat 'sudo usermod -a -G docker jenkins'
       // bat 'npm install newman'
      }
    }
  }
}
