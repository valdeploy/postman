pipeline {
  agent any
    tools {nodejs "node"}
  stages {
    stage('Clean Workspace'){
      steps {
        cleanWs()
      }
    }
    stage ('Checkout') {
      steps {
        checkout scm
      }
    }          
    stage('Setup Postman') {
      steps {
        echo "Test Postman newman"
        sh 'newman run postman/test.postman_collection.json'
        }
        }		
        }
        }
