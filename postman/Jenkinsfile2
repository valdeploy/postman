pipeline {
    agent any
    tools {nodejs "node"}
    stages {
        stage ('Checkout') {
            steps {
                checkout scm
            }
        }
        stage('Build') {
            steps {
                nodejs(nodeJSInstallationName: 'Node 16', configId: '<config-file-provider-id>') {
                    sh 'npm config ls'
                }
            }
        }

    }
}
