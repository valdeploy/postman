pipeline {
  agent any

  tools {nodejs "node"}

  stages {    
 stage ('Checkout') {
            steps {
                checkout scm
            }
        }      
    stage('Install dependencies') {
      steps {
        sh 'npm i -save express'
      }
    }     
    stage('Test') {
      steps {
          sh 'npm config ls'
      }
    }             
  }
}
