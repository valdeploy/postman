pipeline {
  agent any
    stages {     
      stage('Clean Workspace'){
        steps {
          cleanWs()
          }
      }
      stage('Checkout'){
        steps {
          checkout scm
        }
      }
      stage('run Test newman') {
        steps {
          sh 'newman run postman/test.postman_collection.json'
      }
    }
  }
}
