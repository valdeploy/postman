	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"const postRequest = {",
					"  url: 'https://api.uat-asicentral.com/login',",
					"  method: 'POST',",
					"  timeout: 0,",
					"  header: {",
					"    \"Content-Type\": \"application/json\"",
					"  },",
					"  body: {",
					"    mode: 'application/json',",
					"    raw: JSON.stringify(",
					"        {",
					"        \tUserName:pm.collectionVariables.get(\"user\"),",
					"        \tPassword:pm.collectionVariables.get(\"pwd\"),",
					"        \tKickOut:true",
					"        })",
					"  }",
					"};",
					"pm.sendRequest(postRequest, function (err, res) {",
					"    var responseJson = res.json();",
					"    console.log(responseJson);",
					"    pm.collectionVariables.set('bearerToken', responseJson['AccessToken']);",
					"});",
					""
				]
			}
		},
